{
  "name": "client",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "scope": {
    "fields": [
      "name",
      "avatar",
      "id",
      "password",
      "locked"
    ]
  },
  "properties": {
    "uuid": {
      "type": "string",
      "required": true
    },
    "locked": {
      "type": "boolean",
      "default": false
    }
  },
  "validations": [],
  "relations": {},
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "register"
    }
  ],
  "methods": {}
}
